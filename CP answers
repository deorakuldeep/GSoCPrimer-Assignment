Q.1:
#include<iostream>
using namespace std;
int main(){
int a,b;
cin>>a>>b;
cout<<a*b<<endl;
return 0;
}
Q.2:
#include<iostream>
using namespace std;
int main(){
    int a;
    cin>>a;
    if(a%2==0){
        cout<<"The given number is an even number."<<endl;
    }
    else
{
    "The given number is an odd number."<<endl;
}
return 0;
}
Q.7:
#include<iostream>
using namespace std;
int main(){
    int a,b;
    cin>>a>>b;
    int temp;
    temp=a;
    a=b;
    b=temp;
}
Q.10-
#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    cin>>a>>b>>c;
    if(a>b&&a>c){
        cout<<"The maximum number is a"<<endl;
    }
    else if(b>a&&b>c){
        cout<<"The maximum number is b"<<endl;
    }
        else{
            cout<<"The maximum number is c"<<endl;
        }
        return 0;    
}

Q.4-
#include<iostream>
using namespace std;
char str[];
int count=0
for(int i=0;i<length.str();i++){
    if(i=='a'||i=='e'||i=='i'||i=='o'||i=='u'||i=='A'||i=='I'||i=='O'||i=='E'||i=='U')
    {
count++
    }
    return count;
    
}

